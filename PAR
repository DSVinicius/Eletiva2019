#include <stdio.h>

int main(void) {
	int n1, n2, n, soma;
	char nome1[11], nome2[11];
	scanf("%d", &n);
	gets(nome1);
	gets(nome2);
	for(int i=0; i<n; i++){
		scanf("%d", &n1);
		scanf("%d", &n2);
		soma=n1+n2;
		if(soma % 2 == 0){
			printf("%s\n", nome1);
		}else{
			printf("%s\n", nome2);
		}
	}
	printf("\n");
	return 0;
}
